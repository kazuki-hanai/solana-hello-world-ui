    "@testing-library/react": "^11.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/chai": "^4.2.13",
    "@types/classnames": "^2.2.11",
    "@types/eslint": "^7.2.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/snowpack-env": "^2.3.2",
    "@web/test-runner": "^0.10.0",
    "autoprefixer": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.19.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.0.5",
