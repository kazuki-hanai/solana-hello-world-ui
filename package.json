    "classnames": "^2.2.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "tailwindcss": "^2.0.2",
    "tailwindcss-classnames": "^1.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@snowpack/plugin-dotenv": "^2.0.5",
    "@snowpack/plugin-postcss": "^1.0.11",
    "@snowpack/plugin-react-refresh": "^2.3.9",
